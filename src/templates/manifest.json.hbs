{{#with . as | data |}}
{
  "version": "1.0.0",
  "slide": {
    "aspect": "16:9"
  },
  "meta": {
    "name": "{{data.name}}",
    "filename": "{{data.kebab}}",
    "component": "{{data.pascal}}"
  },
  "elements": {
    "componentExample": {
      "type": "text",
      "value": "Hello World",
      "label": "Title"
    }
  }
}
{{/with}}