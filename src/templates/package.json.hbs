{{#with . as | data |}}
{
  "name": "scrowl-slide-template-{{{data.kebab}}}",
  "main": "./dist/index.js",
  "dev": "./dev/index.html",
  "types": "./dist/types.d.ts",
  "targets": {
    "main": {
      "source": "./src/index.tsx",
      "context": "browser"
    },
    "dev": {
      "source": "./dev/index.html"
    }
  },
  "scripts": {
    "update": "scrowl-builder update",
    "watch": "scrowl-builder watch",
    "serve": "parcel serve --target dev --no-cache --open",
    "start": "concurrently --raw \"npm run serve\" \"npm run watch\""
  },
  "devDependencies": {
    "@owlui/lib": "^0.0.29",
    "@parcel/core": "2.5.0",
    "@parcel/packager-ts": "2.5.0",
    "@parcel/reporter-dev-server": "2.5.0",
    "@parcel/transformer-sass": "2.5.0",
    "@parcel/transformer-typescript-types": "2.5.0",
    "@parcel/validator-typescript": "2.5.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "parcel": "2.5.0",
    "postcss": "^8.4.16",
    "postcss-modules": "^4.3.1",
    "react": "~18.1.0",
    "react-bootstrap": "2.4.0",
    "react-dom": "~18.1.0",
    "typescript": "^4.7.4"
  }
}
{{/with}}